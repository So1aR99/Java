        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../images/javafx.png" />
            </image>
        </ImageView>
        <HBox alignment="CENTER" spacing="20.0">
